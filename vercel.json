{
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password",
      "DB_NAME": "@db_name",
      "DB_HOST": "@db_host",
      "JWT_SECRET": "@jwt_secret"
    }
  }
